<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_name, Genre.all, :name, :name %>

  <%= f.text_field :genre_name, list: "genres_autocomplete" %>
<datalist id="genres_autocomplete">
  <% Genre.all.each do |genre| %>
    <option value="<%= genre.name %>">
  <% end %>
</datalist>

<input name="song[notes_ids][]">
<input name="song[notes_ids][]">


  <%= f.submit %>
<% end %>


